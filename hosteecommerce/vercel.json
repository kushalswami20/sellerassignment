{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "./index.js",
      "use": "@vercel/static"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
}
